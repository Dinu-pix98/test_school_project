<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/styles.css">
    <title>Student Marks</title>
</head>
<body>
    <div class="container">
        <h1>Your Marks</h1>
        <table>
            <thead>
                <tr>
                    <th>Subject</th>
                    <th>Marks</th>
                </tr>
            </thead>
            <tbody>
                <% marks.forEach(mark => { %>
                    <tr>
                        <td><%= mark.subject %></td>
                        <td><%= mark.score %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
        <a href="/student/timetable" class="btn">View Timetable</a>
    </div>
    <script src="/js/scripts.js"></script>
</body>
</html>