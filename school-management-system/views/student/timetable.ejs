<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/styles.css">
    <title>Timetable</title>
</head>
<body>
    <div class="container">
        <h1>Your Timetable</h1>
        <table>
            <thead>
                <tr>
                    <th>Day</th>
                    <th>Subject</th>
                    <th>Time</th>
                </tr>
            </thead>
            <tbody>
                <% timetable.forEach(item => { %>
                    <tr>
                        <td><%= item.day %></td>
                        <td><%= item.subject %></td>
                        <td><%= item.time %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
        <a href="/student/marks">View Marks</a>
    </div>
    <script src="/js/scripts.js"></script>
</body>
</html>